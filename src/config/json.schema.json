{
  "$id": "/",
  "type": "array",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
      "items": {
        "$id": "/properties/config/items",
        "type": "object",
        "properties": {
          "title": {
            "$id": "/properties/config/items/properties/title",
            "type": "string",
            "title": "The Title Schema",
            "default": "",
            "examples": [
              "General"
            ]
          },
          "wiki": {
            "$id": "/properties/config/items/properties/wiki",
            "type": "string",
            "title": "Wiki URL for Property",
            "default": "",
            "examples": [
              "/wiki/general"
            ]
          },
          "description": {
            "$id": "/properties/config/items/properties/description",
            "type": "string",
            "title": "Description of menu item",
            "default": ""
          },
          "hasfiles": {
            "$id": "/properties/config/items/properties/hasfiles",
            "type": "boolean",
            "title": "Showing Has SubFiles",
            "default": false,
            "examples": [
              true
            ]
          },
          "options": {
            "$id": "/properties/config/items/properties/options",
            "type": "array",
            "items": {
              "$id": "/properties/config/items/properties/options/items",
              "type": "object",
              "properties": {
                "title": {
                  "$id": "/properties/config/items/properties/options/items/properties/title",
                  "type": "string",
                  "title": "The Title Schema",
                  "default": "",
                  "examples": [
                    "BaseCurrency"
                  ]
                },
                "type": {
                  "$id": "/properties/config/items/properties/options/items/properties/type",
                  "type": "string",
                  "title": "The Type Schema",
                  "default": "",
                  "examples": [
                    "Dropdown"
                  ]
                },
                "options": {
                  "$id": "/properties/config/items/properties/options/items/properties/options",
                  "type": "array",
                  "items": {
                    "$id": "/properties/config/items/properties/options/items/properties/options/items",
                    "type": "object",
                    "properties": {
                      "key": {
                        "$id": "/properties/config/items/properties/options/items/properties/options/items/properties/key",
                        "type": "string",
                        "title": "The Key Schema",
                        "default": "",
                        "examples": [
                          "GBP"
                        ]
                      },
                      "text": {
                        "$id": "/properties/config/items/properties/options/items/properties/options/items/properties/text",
                        "type": "string",
                        "title": "The Text Schema",
                        "default": "",
                        "examples": [
                          "GBP"
                        ]
                      },
                      "value": {
                        "$id": "/properties/config/items/properties/options/items/properties/options/items/properties/value",
                        "type": "string",
                        "title": "The Value Schema",
                        "default": "",
                        "examples": [
                          "GBP"
                        ]
                      }
                    }
                    ,"required":["key", "text", "value"]
                  }
                }
              },
              "required":["title", "type"]
            }
          },
          "advanced": {
            "$id": "/properties/config/items/properties/advanced",
            "type":"boolean",
            "default":false
          }
        },
        "required":["title", "options"],
        "additionalProperties": false
      },
      "additionalProperties": false
}
